test("FormValueTextArea.LongText#1")
{
	HTML_Element* text_elm = find_element("textarea", 1);
	verify(text_elm);
	FormValue* text_value = text_elm->GetFormValue();
	verify(text_value);
	verify(text_value->GetType() == FormValue::VALUE_TEXTAREA);
	FormValueTextArea* text_text_value = FormValueTextArea::GetAs(text_value);
	verify(text_text_value);
	OpString text;
	verify(OpStatus::IsSuccess(text_value->GetValueAsText(text_elm, text)));
	verify(text.CStr());
	verify(text.Length() > 64*1024);
	verify((size_t)text.Length() == 64*1024 +
		   op_strlen("This is the first text after 64KB"));
	OpString expected_text;
	for (int i = 0; i < 1024; i++)
	{
		if (i != 512)
		{
			verify(OpStatus::IsSuccess(expected_text.Append("0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef")));
		}
		else
		{
			verify(OpStatus::IsSuccess(expected_text.Append("\r\n23456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef")));
		}
	}
	verify(OpStatus::IsSuccess(expected_text.Append("This is the first text after 64KB")));
	verify(uni_str_eq(text.CStr(), expected_text.CStr()));
}
