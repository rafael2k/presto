<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <!-- circular dependency -->
  <xsl:variable name="myvar" select="$othervar"/>
  <xsl:variable name="othervar" select="$myvar"/>
  <xsl:template match="/">
    <xsl:value-of select="$myvar"/>
  </xsl:template>
</xsl:stylesheet>
