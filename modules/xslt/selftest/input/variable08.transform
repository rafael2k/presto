<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  <xsl:variable name="myP">P</xsl:variable>
  <xsl:variable name="myA" select="'A'"/>
  <xsl:template match="root">
    <xsl:variable name="myS" select="'S'"/>
    <xsl:text></xsl:text><xsl:value-of select="concat($myP,$myA)"/><xsl:value-of select="concat(($myS),($myS))"/>
  </xsl:template>
</xsl:stylesheet>
