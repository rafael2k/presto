API_LIBOPEAY_HASH_SHA1	haavardm
	Import SHA1 from libopeay.

	Import If: CRYPTO_HASH_SHA1_USE_CORE_IMPLEMENTATION and !FEATURE_NATIVE_SSL
	Import If: CRYPTO_CERTIFICATE_VERIFICATION_USE_CORE_IMPLEMENTATION and !FEATURE_NATIVE_SSL

API_LIBOPEAY_HASH_MD5	haavardm
	Import MD5 from libopeay.

	Import If: CRYPTO_HASH_MD5_USE_CORE_IMPLEMENTATION and !FEATURE_NATIVE_SSL

