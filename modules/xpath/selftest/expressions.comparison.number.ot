{
  "<?xml version='1.0'?>"
  "<!DOCTYPE root>"
  "<root>"
    "<boolean>"
      "<one><yes/></one>"
      "<zero/>"
    "</boolean>"
    "<string>"
      "<minus-one> -1 </minus-one>"
      "<zero> 0 </zero>"
      "<one> 1 </one>"
    "</string>"
    "<nodeset>"
      "<number less-than-minus-one='' less-than-zero='' less-than-one=''> -2 </number>"
      "<number equal-to-minus-one='' less-than-zero='' less-than-one=''> -1 </number>"
      "<number greater-than-minus-one='' equal-to-zero='' less-than-one=''> 0 </number>"
      "<number greater-than-minus-one='' greater-than-zero='' equal-to-one=''> 1 </number>"
      "<number greater-than-minus-one='' greater-than-zero='' greater-than-one=''> 2 </number>"
    "</nodeset>"
  "</root>"
}

