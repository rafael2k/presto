test("Setup")
{
	compareRects = function compareRects(r1, r2, epsilon, id) {
		if (!((Math.abs(r1.x-r2.x) <= epsilon) &&
			  (Math.abs(r1.y-r2.y) <= epsilon) &&
			  (Math.abs(r1.width-r2.width) <= epsilon) &&
			  (Math.abs(r1.height-r2.height) <= epsilon)))
			throw "["+id+"] Rects are not equal. ["+r1.x+","+r1.y+","+r1.width+","+r1.height+"] != ["+r2.x+","+r2.y+","+r2.width+","+r2.height+"]";
		return true;
	}

	makeRect = function makeRect(x,y,w,h)
	{
		var r = document.getElementById("svgroot").createSVGRect();
		r.x = x;
		r.y = y;
		r.width = w;
		r.height = h;
		return r;
	}

	makeScreenRect = function makeScreenRect(id)
	{
		var p = document.getElementById("svgroot").createSVGPoint();
		var r = document.getElementById("svgroot").createSVGRect();
		var ctm = document.getElementById(id).getScreenCTM();
		var bbox = document.getElementById(id).getBBox();
		var corners = new Array();

		p.x = bbox.x;
		p.y = bbox.y;
		corners.push(p.matrixTransform(ctm));

		p.x = bbox.x + bbox.width;
		p.y = bbox.y;
		corners.push(p.matrixTransform(ctm));

		p.x = bbox.x + bbox.width;
		p.y = bbox.y + bbox.height;
		corners.push(p.matrixTransform(ctm));

		p.x = bbox.x;
		p.y = bbox.y + bbox.height;
		corners.push(p.matrixTransform(ctm));

		var minx = corners[0].x;
		var miny = corners[0].y;
		var maxx = corners[0].x;
		var maxy = corners[0].y;

		for(var i = 0; i < corners.length; i++)
		{
			if(corners[i].x < minx)
				minx = corners[i].x;
			else if(corners[i].x > maxx)
				maxx = corners[i].x;
			if(corners[i].y < miny)
				miny = corners[i].y;
			else if(corners[i].y > maxy)
				maxy = corners[i].y;
		}

		r.x = minx;
		r.y = miny;
		r.width = maxx-minx;
		r.height = maxy-miny;

		return r;
	}

	// bboxes
	sb = makeRect(0,0,200,50);
	gb = makeRect(0,-20,190,158);
	cb = makeRect(20,20,20,20);
	rb = makeRect(60,-10,100,35);

	pb = makeRect(240,246,235,100);
	plb = makeRect(0,-20,100,70);
	pgb = makeRect(100,40,180,60);
	eb = makeRect(40,46,20,8);

	ib = makeRect(75,30,75,75);
	fb = makeRect(-10,-10,30,100);
	anb = makeRect(70,45,50,76);
	lb = makeRect(0,0,280,100);

	tb = makeRect(5,63,136,76);
	tiny_tb = makeRect(5,63,50,28); // no textpath support
	tsb = makeRect(31,63,24,9);
	tpb = makeRect(90,93,52,46);
	trb = makeRect(5,83,31,8);

	tab = makeRect(180,10,100,68);
	ub = makeRect(200+40,50+46,20,8); // (200, 50) is the additional translation
	ab = makeRect(240,96,20,8);
	vb = makeRect(30,40,36,57);

	// screen bboxes
	ssb = makeScreenRect("s1");
	sgb = makeScreenRect("g1");
	scb = makeScreenRect("c1");
	srb = makeScreenRect("r1");

	spb = makeScreenRect("p1");
	splb = makeScreenRect("pl1");
	spgb = makeScreenRect("pg1");
	seb = makeScreenRect("e1");

	sib = makeScreenRect("i1");
	sfb = makeScreenRect("f1");
	sanb = makeScreenRect("an1");
	slb = makeScreenRect("l1");

	stb = makeScreenRect("t1");
	tiny_stb = makeScreenRect("t1"); // no textpath support
	stsb = makeScreenRect("ts1");
	stpb = makeScreenRect("tp1");
	strb = makeScreenRect("tr1");

	stab = makeScreenRect("ta1");
	sub = makeScreenRect("u1");
	sab = makeScreenRect("a1");
	svb = makeScreenRect("v1");
}

test("getBBox on visible elements")
	require success "Setup";
	require SVG_FULL_12;
	require SVG_SUPPORT_MEDIA;
{
	var epsilon = 0.5;

	var vals = [ ["s1", sb],
				 ["g1", gb],
				 ["c1", cb],
				 ["r1", rb],

				 ["p1", pb],
				 ["pl1", plb],
				 ["pg1", pgb],
				 ["e1", eb],

				 ["i1", ib],
				 ["f1", fb],
				 ["an1", anb],
				 ["l1", lb],

				 ["t1", tb],
				 ["ts1", tsb],
				 ["tp1", tpb],
				 ["tr1", trb],

				 ["ta1", tab],
				 ["u1", ub],
				 ["a1", ab],
				 ["v1", vb]
			   ];

	for(var i in vals)
	{
		verify(compareRects(document.getElementById(vals[i][0]).getBBox(), vals[i][1], epsilon, vals[i][0]));
	}
}

