xml {
//! <svg xmlns="http://www.w3.org/2000/svg" id="svgroot">
//!   <desc>This tests whether the browser returns a proper bbox for a group with elements that have been moved.
//! The browser should show a green rectangle.  Anything else is a failure</desc>
//!   <g id="g">
//!     <rect id="r" x="10" y="10" width="200" height="100" fill="red"/>
//!   </g>
//! </svg>
}

test("Group with elements changing inside")
{
	compareRects = function compareRects(r1, r2, epsilon, id) {
		if (!((Math.abs(r1.x-r2.x) <= epsilon) &&
			  (Math.abs(r1.y-r2.y) <= epsilon) &&
			  (Math.abs(r1.width-r2.width) <= epsilon) &&
			  (Math.abs(r1.height-r2.height) <= epsilon)))
			throw "["+id+"] Rects are not equal. ["+r1.x+","+r1.y+","+r1.width+","+r1.height+"] != ["+r2.x+","+r2.y+","+r2.width+","+r2.height+"]";
		return true;
	}

	makeRect = function makeRect(x,y,w,h)
	{
		var r = document.getElementById("svgroot").createSVGRect();
		r.x = x;
		r.y = y;
		r.width = w;
		r.height = h;
		return r;
	}

	var epsilon = 0.5;
	var expected1 = makeRect(10,10,200,100);
	var expected2 = makeRect(300,300,200,100);
	var svgroot = document.documentElement;
	var r = document.getElementById("r");
	var g = document.getElementById("g");
	var prebox = g.getBBox();
	r.setAttribute("x", "300");
	r.setAttribute("y", "300");

	var postbox = g.getBBox();

	verify(compareRects(prebox, expected1, epsilon));
	verify(compareRects(postbox, expected2, epsilon));
}

svg {
//! <svg id="svgroot" viewBox="0 0 800 600" width="0" height="600" xmlns="http://www.w3.org/2000/svg">
//!     <g id="group1" transform="translate(10, 20)" fill="red" >
//!         <rect id="rect1" x="10" y="10" width="50" height="50"/>
//!         <g id="group2" transform="translate(10, 20)">
//!             <rect id="rect2" x="0" y="10" width="150" height="0"/>
//!             <circle id="circle1" cx="20" cy="20" r="500"/>
//!         </g>
//!     </g>
//! </svg>
}

table Table1(char*, float, float, float, float)
{
  { "svgroot", -460.0, -440.0, 1000.0, 1000.0 },
  { "group1", -470.0, -460.0, 1000.0, 1000.0 },
  { "rect1", 10.0, 10.0, 50.0, 50.0 },
  { "group2", -480.0, -480.0, 1000.0, 1000.0 },
  { "rect2", 0.0, 10.0, 150.0, 0.0 },
  { "circle1", -480.0, -480.0, 1000.0, 1000.0 }
}

foreach (id, x, y, w, h) from Table1
{
  test("BBox of elements inside a viewport of width=0: " + id)
	language c++;
  {
	SVGDOMRect* r = NULL;
	SVGRect expected(x, y, w, h);
	HTML_Element* elm = state.doc->GetDocRoot()->GetElmById(UNI_L(id));
	verify(elm);
	verify(SVGDOM::GetBoundingBox(elm, state.doc, r, SVGDOM::SVG_BBOX_NORMAL) == OpBoolean::IS_TRUE);
	SVGRect returned(r->GetX(), r->GetY(), r->GetWidth(), r->GetHeight());
	BOOL result = expected.IsEqual(returned);
	if (!result)
	{
		output("[%s] Expected: [%g,%g,%g,%g]. Got: [%g,%g,%g,%g] ", 
			id, x, y, w, h,
			r->GetX(), r->GetY(), r->GetWidth(), r->GetHeight());
	}
	verify(result);
  }
  finally
  {
	  OP_DELETE(r);
  }
}

svg {
//! <svg id="svgroot" viewBox="0 0 800 600" width="800" height="600" xmlns="http://www.w3.org/2000/svg">
//!     <g id="group1" transform="translate(10, 20)" fill="red" >
//!         <rect id="rect1" x="10" y="10" width="400" height="0"/>
//!         <rect id="rect2" x="10" y="10" width="0" height="200"/>
//!         <g id="group2" transform="translate(10, 20)">
//!             <circle id="circle1" cx="20" cy="40" r="0"/>
//!         </g>
//!         <ellipse id="ellipse1" cx="50" cy="20" rx="0" ry="40"/>
//!         <ellipse id="ellipse2" cx="50" cy="20" rx="20" ry="0"/>
//!         <animation id="animation1" x="30" y="10" width="0" height="100"/>
//!         <animation id="animation2" x="60" y="30" width="50" height="0"/>
//!         <image id="image1" x="10" y="20" width="90" height="0"/>
//!         <image id="image2" x="0" y="40" width="0" height="20"/>
//!     </g>
//!     <foreignObject id="fo1" x="10" y="20" width="90" height="0"/>
//!     <foreignObject id="fo2" x="0" y="40" width="0" height="20"/>
//!     <video id="video1" x="10" y="20" width="90" height="0"/>
//!     <video id="video2" x="0" y="40" width="0" height="20"/>
//! </svg>
}

table Table2(char*, float, float, float, float, char*)
{
  { "rect1", 10.0, 10.0, 400.0, 0.0, SVG_SUPPORT },
  { "rect2", 10.0, 10.0, 0.0, 200.0, SVG_SUPPORT },
  { "group2", 20.0, 40.0, 0.0, 0.0, SVG_SUPPORT },
  { "ellipse1", 50.0, -20.0, 0.0, 80.0, SVG_SUPPORT },
  { "ellipse2", 30.0, 20.0, 40.0, 0.0, SVG_SUPPORT },
  { "animation1", 30.0, 10.0, 0.0, 100.0, SVG_TINY_12 },
  { "animation2", 60.0, 30.0, 50.0, 0.0, SVG_TINY_12 },
  { "image1", 10.0, 20.0, 90.0, 0.0, SVG_SUPPORT},
  { "image2", 0.0, 40.0, 0.0, 20.0, SVG_SUPPORT },
  { "fo1", 10.0, 20.0, 90.0, 0.0, SVG_SUPPORT },
  { "fo2", 0.0, 40.0, 0.0, 20.0, SVG_SUPPORT },
  { "video1", 10.0, 20.0, 90.0, 0.0, SVG_SUPPORT_MEDIA },
  { "video2", 0.0, 40.0, 0.0, 20.0, SVG_SUPPORT_MEDIA }
}

foreach (id, x, y, w, h, define) from Table2
{
  test("BBox of elements with 0 width or height: " + id)
	language c++;
	require define;
  {
	SVGDOMRect* r = NULL;
	SVGRect expected(x, y, w, h);
	HTML_Element* elm = state.doc->GetDocRoot()->GetElmById(UNI_L(id));
	verify(elm);
	verify(SVGDOM::GetBoundingBox(elm, state.doc, r, SVGDOM::SVG_BBOX_NORMAL) == OpBoolean::IS_TRUE);
	SVGRect returned(r->GetX(), r->GetY(), r->GetWidth(), r->GetHeight());
	BOOL result = expected.IsEqual(returned);
	if (!result)
	{
		output("[%s] Expected: [%g,%g,%g,%g]. Got: [%g,%g,%g,%g] ", 
			id, x, y, w, h,
			r->GetX(), r->GetY(), r->GetWidth(), r->GetHeight());
	}
	verify(result);
  }
  finally
  {
	  OP_DELETE(r);
  }
}
