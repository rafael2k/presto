<?xml version="1.0"?>

<define type="Dialog" name="GoToPageDialog">

  <Dialog>

    <Title string_ref="DI_HENTURL"/>

    <StackLayout>

      <Label id="label_for_Address_field" string_ref="D_GOTOPAGE_ENTER_WEBADDR"/>

      <Choice>
	<Option id="Address Option">
	  <AddressField id="Address_field"/>
	</Option>
	<Option id="Nick Option">
	  <Edit id="Nick_field"/>
	</Option>
      </Choice>
    </StackLayout>

    <ButtonStrip ref="OkCancel">

      <!-- Override the action on the ok button -->
      <Button rel_id="Ok">
	<Action ref="Go to page"/>
      </Button>

    </ButtonStrip>

  </Dialog>

</define>