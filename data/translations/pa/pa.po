#. the zoom to Automatic or Manual: see
#. D_SPEEDDIAL_CONFIG_ZOOM_CONTROL_AUTOMATIC and
#. D_SPEEDDIAL_CONFIG_ZOOM_CONTROL_MANUAL
#. Scope: ui
#: D_SPEEDDIAL_CONFIG_ZOOM_CONTROL:138340236
msgctxt "D_SPEEDDIAL_CONFIG_ZOOM_CONTROL"
msgid "Zoom:"
msgstr "ਜ਼ੂਮ:"

#. In the Speed Dial configuration dialog, a drop-down item that sets the zoom
#. to Automatic, disabling the zoom slider
#. Scope: ui
#: D_SPEEDDIAL_CONFIG_ZOOM_CONTROL_AUTOMATIC:156822834
msgctxt "D_SPEEDDIAL_CONFIG_ZOOM_CONTROL_AUTOMATIC"
msgid "Automatic"
msgstr "ਸਵੈਚਲਿਤ"

#. In the Speed Dial configuration dialog, a drop-down item that sets the zoom
#. to Manual, activating the zoom slider
#. Scope: ui
#: D_SPEEDDIAL_CONFIG_ZOOM_CONTROL_MANUAL:2060691113
msgctxt "D_SPEEDDIAL_CONFIG_ZOOM_CONTROL_MANUAL"
msgid "Manual"
msgstr "ਹੱਥੀਂ"

#. In the Speed Dial configuration dialog, a label for the checkbox that hides
#. the Add Dial button
#. Scope: ui
#: D_SPEEDDIAL_CONFIG_HIDE_ADD_BUTTON:488575268
msgctxt "D_SPEEDDIAL_CONFIG_HIDE_ADD_BUTTON"
msgid "Hide [+] Speed Dial button"
msgstr "ਛੁਪਾਉ [+] ਸਪੀਡ ਡਾਇਲ ਬਟਨ"

