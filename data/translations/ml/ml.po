#: SI_HISTORY_LIST_TEXT_SHORT:1665871264
msgctxt "SI_HISTORY_LIST_TEXT_SHORT"
msgid "History"
msgstr "ചരിത്രം"

#. Option for setting landscape as default in settings.
#. Scope: bream
#: S_BREAM_LANDSCAPE:1213583432
msgctxt "S_BREAM_LANDSCAPE"
msgid "Landscape"
msgstr "ലാന്‍ഡ്സ്കേപ്പ്"

#. Option for setting fullscreen as default in settings.
#. Scope: bream
#: S_BREAM_FULLSCREEN:1675667152
msgctxt "S_BREAM_FULLSCREEN"
msgid "Fullscreen"
msgstr "മുഴുവന്‍സ്ക്രീന്‍"

#. Back. Note to translators: Depending on the device, this string may be
#. truncated, and in the worst case, the first 7 characters should convey the
#. essential idea.
#. Scope: bream
#: SI_PREV_BUTTON_TEXT_SHORT:1551202251
msgctxt "SI_PREV_BUTTON_TEXT_SHORT"
msgid "Back"
msgstr "പിന്നോട്ട്"

#. General menu string. Note to translators: Depending on the device, this
#. string may be truncated, and in the worst case, the first 7 characters
#. should convey the essential idea.
#. Scope: bream
