msgctxt "S_SSL_FATAL_ERROR_MESSAGE"
msgid "Unable to complete secure transaction"
msgstr "Nije moguće obaviti bezbednu transakciju"

#. This is the title of an error page displayed when a URL containing invalid
#. characters in the servername is rejected during URL resolving
#. Scope: core,ezx,base,nfl,nitro
#: S_ILLEGAL_URL_TITLE:70526
msgctxt "S_ILLEGAL_URL_TITLE"
msgid "Invalid URL"
msgstr "Nevažeća URL adresa"

#. First line of the message, the URL is displayed on the next line
#. Scope: core,ezx,base,nfl,nitro
#: S_ILLEGAL_URL_LINE_1:70527
msgctxt "S_ILLEGAL_URL_LINE_1"
msgid "The URL"
msgstr "URL"

#. Second part of the illegal URL message, follows the URL
#. Scope: core,ezx,base,nfl,nitro
#: S_ILLEGAL_URL_LINE_2:70528
msgctxt "S_ILLEGAL_URL_LINE_2"
msgid "contains characters that are not valid in the location they are found."
msgstr "sadrži znakove koji nisu važeći na lokaciji na kojoj se nalaze."

#. The possible reasons for why the invalid characters are there: Mistake, or
#. attack.
#. Scope: core,ezx,base,nfl,chameleon
#: S_ILLEGAL_URL_REASON:70529
msgctxt "S_ILLEGAL_URL_REASON"
msgid "The reason for their presence may be a mistyped URL, but the URL may also be an attempt to trick you into visiting a web site which you might mistake for a site you trust."
msgstr "Razlog za njihovo prisustvo može biti greška u unošenju URL adrese, ali ta URL adresa može biti i pokušaj prevare koji će vas navesti da posetite Web lokaciju za koju pogrešno mislite da je bezbedna."

#. Header for the SSL certificate description page" !!IMPORTANT!! No HTML
#. special characters are allowed !!
#. Scope: core,ssl,not-symbian
#: S_MSG_SSL_TEXT_SECINFO:70530
msgctxt "S_MSG_SSL_TEXT_SECINFO"
