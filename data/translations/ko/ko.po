#. Scope: momail
#: S_FORMAT_DECIMAL_POINT:674899703
msgctxt "S_FORMAT_DECIMAL_POINT"
msgid "."
msgstr "."

#. Symbol used to separate groups of 3 digits (forming the thousands) in large
#. numbers. For example, 'The US national debt is greater than
#. $10,000,000,000. Here the comma is used to separate each group of 3 digits;
#. this is the thousands separator. If numbers are not divided up like this in
#. your language, you should translate this to the empty string.
#. Scope: momail
#: S_FORMAT_THOUSANDS_SEPARATOR:1686021416
msgctxt "S_FORMAT_THOUSANDS_SEPARATOR"
msgid ","
msgstr ","

#. Date format for writing year/month/day in numerical form, e.g.
#. "01/01/1982". Date formats are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html).
#. Scope: momail
#: S_CALENDAR_FORMAT_DATE:1335367894
msgctxt "S_CALENDAR_FORMAT_DATE"
msgid "%m/%d/%Y"
msgstr "%Y/%m/%d"

#. Date format for writing the time in the 24h clock (hours and minutes only),
#. e.g. "13:01". Date formats are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html).
#. Scope: momail
#: S_CALENDAR_FORMAT_TIME_24:939946348
msgctxt "S_CALENDAR_FORMAT_TIME_24"
msgid "%H:%M"
msgstr "%H:%M"

#. Date format for writing the time in the 12h clock (hours and minutes only),
#. e.g. "1:01 PM". Date formats are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html).
#. Scope: momail
#: S_CALENDAR_FORMAT_TIME_12:939946313
msgctxt "S_CALENDAR_FORMAT_TIME_12"
msgid "%-I:%M %p"
msgstr "%-I:%M %p"

#. This should be either "12h" or "24h" depending on whether a 12-hour or
#. 24-hour clock is most commonly used in your locale.
#. Scope: momail
#: S_CALENDAR_FORMAT_TIME_DEFAULT:1425668299
msgctxt "S_CALENDAR_FORMAT_TIME_DEFAULT"
msgid "12h"
msgstr "12h"

#. Date format for writing the full date and time, including day of the week
#. if normally used in your locale, e.g. "Monday, December 04, 1982 2:04 PM".
#. Date formats are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html).
#. Scope: momail
#: S_CALENDAR_FORMAT_FULL_DATE_TIME:2040008502
msgctxt "S_CALENDAR_FORMAT_FULL_DATE_TIME"
msgid "%A, %B %d, %Y %-I:%M %p"
msgstr "%A, %B %d, %Y %-I:%M %p"

#. Date format for writing the date (day and month only) in a reasonably
#. concise manner, using abbreviated versions of months if possible in the
#. language, e.g. "Sep 4". Date formats are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html). The
#. string is treated literally except for variables, which consist of a %
#. sign, optionally followed by a - (hyphen), and then a character. If a
#. hyphen is used, then the number is not padded (with 0s) to a fixed length,
#. e.g. 4/4/1992.
#. Scope: momail
#: S_CALENDAR_FORMAT_SHORT_DAY_MONTH:311318442
msgctxt "S_CALENDAR_FORMAT_SHORT_DAY_MONTH"
msgid "%b %-d"
msgstr "%b %-d"

#. Date format for writing the date (day, month, year) in a reasonably concise
#. manner, using abbreviated names of months if possible in the language, e.g.
#. "Sep 3 '08". If it is not acceptable to write a date like this using month
#. names, instead use numbers as in S_CALENDAR_FORMAT_DATE, e.g. "23/04/1992".
#. Date formats are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html). The
#. string is treated literally except for variables, which consist of a %
#. sign, optionally followed by a - (hyphen), and then a character. If a
#. hyphen is used, then the number is not padded (with 0s) to a fixed length,
#. e.g. 4/4/1992.
#. Scope: momail
#: S_CALENDAR_FORMAT_SHORT_DAY_MONTH_YEAR:1603454394
msgctxt "S_CALENDAR_FORMAT_SHORT_DAY_MONTH_YEAR"
msgid "%b %-d '%y"
msgstr "%b %-d '%y"

#. Date format for writing the day of the week plus the date in a reasonably
#. concise manner, using abbreviated names of days and months if possible in
#. the language, e.g. "Sat, Apr 4, 1992". The full year should be used. If it
#. is not acceptable to write a date like this using month names, instead use
#. numbers as in S_CALENDAR_FORMAT_DATE, e.g. "Sat, 04/04/1992". Date formats
#. are written using strftime format
#. (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html). The
#. string is treated literally except for variables, which consist of a %
#. sign, optionally followed by a - (hyphen), and then a character. If a
#. hyphen is used, then the number is not padded (with 0s) to a fixed length,
#. e.g. 4/4/1992.
#. Scope: momail
#: S_CALENDAR_FORMAT_SHORT_DAY_DATE:9095394
msgctxt "S_CALENDAR_FORMAT_SHORT_DAY_DATE"
msgid "%a, %b %-d, %Y"
msgstr "%a, %b %-d, %Y"

