msgstr "Kliknij Messengera prawym przyciskiem myszy"

#. Stopped Messenger application error message overlay
#. Line 5 of 5
#: templates/index.html
msgid "Select <strong>Start</strong>"
msgstr "Wybierz <strong>Uruchom</strong>"

#. Header used in Friends area before any friends
#. are added
#: templates/index.html
msgid "Get started"
msgstr "Rozpocznij"

#. Message shown while retrieving friend data
#: templates/index.html
msgid "Loading…"
msgstr "Ładowanie…"

#. Header and control for showing the Help page
#: templates/index.html
msgid "Help"
msgstr "Pomoc"

#. Date range filter line item
#. Clicking the control shows messages from that point onward
#: templates/index.html
msgid "Older messages:"
msgstr "Starsze wiadomości:"

#. Date range filter line item
#. Clicking the control shows messages from that point onward
#: templates/index.html
msgid "Today"
msgstr "Dzisiaj"

#. Date range filter line item
#. Clicking the control shows messages from that point onward
#: templates/index.html
msgid "Yesterday"
msgstr "Wczoraj"

#. Date range filter line item
#. Clicking the control shows messages from that point onward
#: templates/index.html
msgid "Last week"
msgstr "Ostatni tydzień"

#. Date range filter line item
#. Clicking the control shows messages from that point onward
#: templates/index.html
msgid "Last month"
