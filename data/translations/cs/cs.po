#. Error message that the config.xml of an uploaded package file is in the
#. wrong location in the package.
#. Scope: ironmaiden
#: S_ERROR_CONFIG_NOT_IN_ROOT:673560767
msgctxt "S_ERROR_CONFIG_NOT_IN_ROOT"
msgid "The configuration file must be at the root of the archive"
msgstr "Soubor s nastavením musí být v kořenové složce archívu"

#. Error message that the config.xml of an uploaded Opera Widget package file
#. is in the wrong location in the package.
#. Scope: ironmaiden
#: S_ERROR_CONFIG_NOT_IN_ROOT_SINGLE_DIR:668569886
msgctxt "S_ERROR_CONFIG_NOT_IN_ROOT_SINGLE_DIR"
msgid "The configuration file must be at the root of or inside a single root directory of the archive"
msgstr "Soubor s nastavením musí být v kořenové složce či uvnitř jediného kořenového adresáře archívu"

#. Error message for the rating form when the rating is not supplied.
#. Scope: ironmaiden
#: S_ERROR_RATING_REQUIRED:432077541
msgctxt "S_ERROR_RATING_REQUIRED"
msgid "Please give a rating."
msgstr "Prosím, zadejte hodnocení."

#. Error message for the rating form when the rating supplied is invalid.
#. Scope: ironmaiden
#: S_ERROR_RATING_SELECT:242448548
msgctxt "S_ERROR_RATING_SELECT"
msgid "Please select one of the five stars."
msgstr "Prosím, vyberte některou z pěti hvězdiček."

#. Label for posting as anonymous in the report form for a package.
#. Scope: ironmaiden
#: S_LABEL_ANONYMOUS:1827601503
msgctxt "S_LABEL_ANONYMOUS"
msgid "Anonymous"
msgstr "Anonymní"

#. Error message that the name supplied in an uploaded package file is already
#. in use for another package.
#. Scope: ironmaiden
#: S_ERROR_PACKAGE_NAME_EXISTS:1395031051
msgctxt "S_ERROR_PACKAGE_NAME_EXISTS"
msgid "A package with this name already exists. Please change the name or upgrade the existing package."
msgstr "Stejnojmenný balíček již existuje. Vyberte jiné jméno nebo zaktualizujte již existující balíček."

#. Error message that the config.xml of an uploaded package file does not
#. follow the specification.
#. Scope: ironmaiden
#: S_ERROR_INVALID_CONFIG:330509084
msgctxt "S_ERROR_INVALID_CONFIG"
msgid "Your config.xml does not comply with the W3C <a href=\"http://www.w3.org/TR/widgets/\">Widget Packaging and XML Configuration</a> specification. Please correct it and try uploading again."
msgstr "Váš soubor config.xml neodpovídá specifikaci W3C pro <a href=\"http://www.w3.org/TR/widgets/\">balíčkování pomůcek a nastavení XML</a>. Prosím, opravte jej a znovu nahrajte na server."

#. Error message that the config.xml of an uploaded package file has syntax
#. errors.
#. Scope: ironmaiden
#: S_ERROR_CONFIG_SYNTAX:602021852
msgctxt "S_ERROR_CONFIG_SYNTAX"
msgid "There is a syntax error in your config.xml file. Please correct it and try uploading again."
msgstr "Ve vašem souboru config.xml je syntaktická chyba. Prosím, opravte ji a poté soubor znovu nahrajte na server."

#. Generic error message when uploading a package file failed.
#. Scope: ironmaiden
#: S_ERROR_UPLOAD_FAILED:2054722953
msgctxt "S_ERROR_UPLOAD_FAILED"
msgid "There was a problem uploading your file. Please try again later."
msgstr "Vyskytl se problém s nahráním vašeho souboru. Prosím, opakujte akci později."

#. Error message that an uploaded package file contains a config.xml with an
#. unknown namespace.
#. Scope: ironmaiden
#: S_ERROR_UNKNOWN_NAMESPACE:1796510492
msgctxt "S_ERROR_UNKNOWN_NAMESPACE"
msgid "Your config.xml uses an unknown namespace. Please use the namespace 'http://www.w3.org/ns/widgets' as defined in the W3C <a href=\"http://www.w3.org/TR/widgets/\">Widget Packaging and XML Configuration</a> specification."
msgstr "Váš soubor config.xml používá neznámý jmenný prostor. Prosím, použijte jmenný prostor 'http://www.w3.org/ns/widgets' definovaný ve specifikaci W3C pro <a href=\"http://www.w3.org/TR/widgets/\">balíčkování pomůcek a nastavení XML</a>."

#. Error message the an uploaded icon does not have the correct size in the
#. Artwork icon form in the package uploader.
#. Scope: ironmaiden
#: S_ERROR_ICON_INCORECT_SIZE:343672217
msgctxt "S_ERROR_ICON_INCORECT_SIZE"
msgid "The icon you uploaded did not have the correct size"
msgstr "Ikona nahraná na server nemá správnou velikost"

