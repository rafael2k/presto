#. The title of the Applications-tab.
#: TO BE ADDED
msgid "Applications"
msgstr "Aplicaciones"

#. The title of the Account-tab.
#: TO BE ADDED
msgid "Account"
msgstr "Cuenta"

#. A description of the Home application page and the list that shows the owner's running applications.
#: TO BE ADDED
msgid "This page lists applications running on {username}'s computer. Click for direct access to the content."
msgstr "En esta página está la lista de aplicaciones ejecutándose en el PC de {username}. Se hace clic para acceder a los contenidos."

#. A description of the message an owner can leave for his visitors.
#: TO BE ADDED
msgid "Leave a message that your friends will see when they visit your Opera Unite applications."
msgstr "Deja un mensaje para los amigos que visiten tus aplicaciones de Opera Unite."

#. A submit button for a form to save a personal message for the visitors.
#: TO BE ADDED
msgid "Save"
msgstr "Guardar"

#. A cancel button for a form to cancel editing a personal message for the visitors.
#: TO BE ADDED
msgid "Cancel"
msgstr "Cancelar"

#. A link to the owner's applications
#: TO BE ADDED
msgid "My applications"
msgstr "Mis aplicaciones"

#. A link to the owner's friends' applications
#: TO BE ADDED
msgid "My friends' applications"
msgstr "Las aplicaciones de mis amigos"

#. Heading for a list of the owner's running applications.
#: TO BE ADDED
msgid "{username}'s applications"
msgstr "Aplicaciones de {username}"

#. Heading for a list of the owner's friends' running applications.
#: TO BE ADDED
msgid "{username}'s friends' applications"
msgstr "Aplicaciones de los amigos de {username}"

#. A message for the owner when no applications are running.
#: TO BE ADDED
msgid "Start using Opera Unite applications to share files, create instant photo galleries, listen to your music from anywhere, and much, much more. You can start these applications from the <A href=\"http://unite.opera.com/support/userguide/#walkthro\">Unite panel</A> and <A href=\"http://unite.opera.com/applications\">download more</A> absolutely free!"
msgstr "Comienza a usar aplicaciones de Opera Unite para compartir archivos, crear galerías de fotos en un momento, escuchar tu música desde cualquier lugar y mucho, mucho más. Puedes ejecutar las aplicaciones desde el <A href=\"http://unite.opera.com/support/userguide/#walkthro\">Panel de Unite</A> y  <A href=\"http://unite.opera.com/applications\">descargar más</A> completamente gratis."

#. A link to launch {serviceName}
#. Eg. 'Launch Media Player', 'Launch File Sharing'
#: TO BE ADDED
msgid "Launch {serviceName}"
msgstr "Iniciar {serviceName}"

#. A message for a visitor when no applications are running.
#: TO BE ADDED
msgid "{username} is not running any applications at the moment."
msgstr "{username} no está ejecutando ninguna aplicación en este momento."

#. An action button that opens an application. It is showed next to each
#. application in the list of applications.
#: TO BE ADDED
msgid "Visit"
msgstr "Visitar"

#. An link button that goes to unite.opera.com/applications/ for the owner
#. to get more applications.
#: TO BE ADDED
msgid "Get More Applications"
msgstr "Conseguir más aplicaciones"

#. A heading for a section that contains the recent activity on all other applications.
#: TO BE ADDED
msgid "Recent activity"
msgstr "Actividad reciente"

#. A link that goes to a page that displays all activity from other applications.
#: TO BE ADDED
msgid "View all"
msgstr "Ver todas"

#. A message that displays activity from the Messenger application.
#. Eg. 'Message received from sjogren in AppName 3 minutes ago.'
#: TO BE ADDED
msgid "Message received from <A href=\"{link}\">{userName}</A> in <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "Mensaje recibido de <A href=\"{link}\">{userName}</A> en <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the File Sharing application.
#. Eg. 'file.mp3 was downloaded from AppName 6 minutes ago.'
#: TO BE ADDED
msgid "<A href=\"{link}\">{filename}</A> was downloaded from <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "<A href=\"{link}\">{filename}</A> se descargó desde <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the Web Server application.
#. Eg. 'index.html was served by AppName 6 minutes ago.'
#: TO BE ADDED
msgid "<A href=\"{link}\">{filename}</A> was served by <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "<A href=\"{link}\">{filename}</A> fue enviado por <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the Photo Sharing application.
#. Eg. 'photo.jpg was viewed in AppName 11 minutes ago.'
#: TO BE ADDED
msgid "<A href=\"{link}\">{filename}</A> was viewed in <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "<A href=\"{link}\">{filename}</A> fue visto en <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the Photo Sharing application.
#. Eg. 'A slideshow of Summer Vacation 2009 was started in AppName 21 minutes ago.'
#: TO BE ADDED
msgid "A slideshow of <A href=\"{link}\">{albumName}</A> was started in <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "Se inició una presentación de <A href=\"{link}\">{albumName}</A> en <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the Media Player application.
#. Eg. 'Michael Jackson - Beat it was played by AppName 2 minutes ago.'
#: TO BE ADDED
msgid "<A href=\"{link}\">{filename}</A> was played by <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "<A href=\"{link}\">{filename}</A> fue reproducido por <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the Lounge application.
#. Eg. 'Anders joined AppName 13 minutes ago.'
#: TO BE ADDED
msgid "<A href=\"{link}\">{userName}</A> joined <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "<A href=\"{link}\">{userName}</A> se unió a <A href=\"{appLink}\">{appName}</A> {relativeTime}."

#. A message that displays activity from the Fridge application.
#. Eg. 'Anders posted a note on AppName 6 minutes ago.'
#: TO BE ADDED
msgid "<A href=\"{link}\">{userName}</A> posted a note on <A href=\"{appLink}\">{appName}</A> {relativeTime}."
msgstr "<A href=\"{link}\">{userName}</a> publicó una nota en <A href=\"{appLink}\">{appName}</A> {relativeTime}."

