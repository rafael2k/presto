#: SI_ERR_MAX_CONNECTIONS_SERVER_SETTING:20018
msgctxt "SI_ERR_MAX_CONNECTIONS_SERVER_SETTING"
msgid "The maximum connections to a single server must be between 1 and 99."
msgstr "El número máximo de conexiones a un mismo servidor es 99."

#. Scope: core,unsorted
#: SI_ERR_MAX_CONNECTIONS_TOTAL_SETTING:20019
msgctxt "SI_ERR_MAX_CONNECTIONS_TOTAL_SETTING"
msgid "The maximum total connections must be between 1 and 999."
msgstr "El máximo total de conexiones debe estar entre 1 y 999."

#. Scope: core,unsorted
#: SI_ERR_ILLEGAL_NUMBER_HISTORY_LINES:20020
msgctxt "SI_ERR_ILLEGAL_NUMBER_HISTORY_LINES"
msgid "The number of history lines must be set between 0 and 999."
msgstr "El número de líneas en historial debe estar entre 0 y 999."

#. Used in a popup error message when the user has requested a file from the
#. server, and the server recieves the request but no data exists to return
#. (HTTP error code 204), if the user has set the Network/Server errors/Always
#. show dialog box preference.
#. Scope: core,base,ezx,brew,nitro,nfl
#: SI_ERR_HTTP_NO_CONTENT:33044
msgctxt "SI_ERR_HTTP_NO_CONTENT"
msgid "Remote server accepted the request - no content returned"
msgstr "El servidor ha aceptado la petición - no ha devuelto contenidos"

#. Used in a popup error message when the user has entered a file URL in the
#. URL field and the file is read-protected.
#. Scope: core,base,ezx,brew,nitro,nfl
#: SI_ERR_ACCESS_DENIED:32903
msgctxt "SI_ERR_ACCESS_DENIED"
msgid "Access denied to this file"
msgstr "240007 : Se ha denegado el acceso a este archivo."

