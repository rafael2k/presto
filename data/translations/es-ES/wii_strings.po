#: SI_ERR_XML_OUTOFMEMORY:25001
msgctxt "SI_ERR_XML_OUTOFMEMORY"
msgid "out of memory"
msgstr "memoria insuficiente"

#. Used in a warning/confirmation dialog when an URL with a user name (like
#. http://username@www.example.com) is entered in the URL field.
#. Scope: core,base,ezx,brew,nitro,chameleon
#: SI_MSG_WARN_USERNAME_IN_URL:21463
#, c-format
msgctxt "SI_MSG_WARN_USERNAME_IN_URL"
msgid ""
"Security warning:\n"
"\n"
"You are about to go to an address containing a username.\n"
"\n"
"Username: %s \n"
"Server: %s\n"
"\n"
"Are you sure you want to go to this address?"
msgstr ""
"240008 : Advertencia de seguridad:\n"
"\n"
"Estás a punto de ir a una dirección que contiene un nombre de usuario.\n"
"\n"
"    Usuario: %s \n"
"    Servidor: %s\n"
"\n"
"¿Seguro que quieres ir a esta dirección?"

#. The error page that is shown when loading a URL fails.
#. Scope: ui,nfl,symbian,hokkaido
#: S_LOADINGFAILED_PAGE_HELP_HEADER:70035
#, c-format
msgctxt "S_LOADINGFAILED_PAGE_HELP_HEADER"
msgid "You tried to access the address %s, which is currently unavailable. Please make sure that the Web address (URL) is correctly spelled and punctuated, then try reloading the page."
msgstr "La dirección a la que has intentado acceder, %s, no está disponible en este momento. Asegúrate de que la dirección de la página web (URL) está escrita correctamente y a continuación actualiza la página."

